{
  "name": "function-plot",
  "version": "1.22.8",
  "description": "A simple 2d function plotter powered by d3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriciopoppe/function-plot"
  },
  "bugs": "https://github.com/mauriciopoppe/function-plot/issues",
  "homepage": "http://mauriciopoppe.github.io/function-plot/",
  "author": "Mauricio Poppe <mauricio.poppe@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "deploy": "gh-pages-deploy",
    "html": "node site.js",
    "prepare": "npm run build",
    "build": "npx rimraf dist && npm run html && npm run docs && npm run build:types && npm run build:webpack && npm run build:site",
    "build:webpack": "NODE_ENV=production webpack",
    "build:types": "tsc",
    "build:site": "cp dist/function-plot.js site/js/function-plot.js",
    "start": "webpack-dev-server",
    "docs": "typedoc --out site/docs src/index.ts",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic"
  },
  "files": [
    "/dist"
  ],
  "gh-pages-deploy": {
    "prep": [
      "build"
    ],
    "staticpath": "site"
  },
  "keywords": [
    "function-plot",
    "function",
    "plotter",
    "visualization",
    "derivative",
    "2d"
  ],
  "standard": {
    "ignore": [
      "sandbox/",
      "site/",
      "dist/"
    ]
  },
  "dependencies": {
    "built-in-math-eval": "^0.3.0",
    "clamp": "^1.0.1",
    "d3-axis": "^2.0.0",
    "d3-color": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.2",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "interval-arithmetic-eval": "^0.4.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/d3": "^5.7.2",
    "@types/events": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chromatic": "^5.2.0",
    "core-js": "^3.6.5",
    "dox": "^0.9.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages-deploy": "^0.5.1",
    "html-webpack-plugin": "^5.5.0",
    "jade": "^1.9.2",
    "jest": "^26.4.2",
    "lodash": "^4.17.11",
    "markdown-it": "^11.0.1",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.21.0",
    "react-is": "^16.13.1",
    "react-simple-code-editor": "^0.11.0",
    "standard": "^12.0.1",
    "standardx": "^5.0.0",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.23.8",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
