{
    "root": true,
    "extends": ["react-app", "standard"],
    "rules": {
        "space-before-function-paren": ["error", {
            "anonymous": "ignore",
            "named": "ignore",
            "asyncArrow": "ignore"
        }],
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}
